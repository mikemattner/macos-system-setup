cmd + alt - return : open -a 'iTerm'
cmd + alt - m : open -a 'Sublime Merge'
cmd + alt - x : open -a 'Slack'
cmd + alt - v : open -a 'Visual Studio Code'
cmd + alt - z : open -a 'zoom.us'
cmd + alt - p : open -a 'Perplexity'
cmd + alt - f : open -a 'Firefox'
cmd + alt - c : open -a 'Google Chrome'
cmd + alt - s : open -a 'Safari'
cmd + alt - t : open -a 'Todoist'
cmd + alt - n : open -a 'Notes'